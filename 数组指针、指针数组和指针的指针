#include <iostream>
#include <string>

using namespace std;

int main()
{
    //https://blog.csdn.net/sszgg2006/article/details/7317877
	//https://blog.csdn.net/qsyzb/article/details/9567463
	double(*arr1)[6] = new double[10][6];  // 数组指针
	double *arr2[6];   // 指针数组
	double arr3[10][6]; // 数组数组
	double **arr4 = new double*[10];  //指针的指针

	return 0;
}
